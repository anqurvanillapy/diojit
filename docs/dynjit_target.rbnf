absvalue ::= ( repr ','  type ) ;

expr ::= absvalue | 'call' expr ( expr '*' );

stmt ::= var '=' expr
     |   'goto' '$\\mbold {label}$'
     |   'return' expr
     |   'if' expr 'goto' '$\\mbold {label}$' 'goto' '$\\mbold {label}$'
     |   'do' expr
     |   'label' '$\\mbold {label}$'
     |   '{' stmts '}'
     |   'switch' expr case '*'
     ;

case ::= '|' type '$\\rightarrow$' stmt ;

stmts ::= stmt '*' ;


